{application, raven, [
	{description, "Erlang client for Sentry"},
	{vsn, git},
	{registered, []},
	{applications, [
		kernel,
		stdlib,
		crypto,
		public_key,
		ssl,
		inets,
		jsone
	]},
	{mod, {raven_app, []}},
	{env, [
		{dsn, "https://PUBLIC_KEY:PRIVATE_KEY@app.getsentry.com/PROJECT_ID"},
		{error_logger, false},
		{ipfamily, inet}
	]}
]}.
